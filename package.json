{
  "name": "jhtmls",
  "title": "jhtmls",
  "description": "JS and HTML alternate javascript template",
  "version": "1.0.2",
  "homepage": "http://jhtmls.com/",
  "main": "jhtmls.js",
  "author": [
    {
      "name": "zswang",
      "url": "http://weibo.com/zswang"
    },
    {
      "name": "zinkey",
      "url": "http://weibo.com/zinkey"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zswang/jhtmls.git"
  },
  "keywords": [
    "template",
    "javascript",
    "html"
  ],
  "bugs": {
    "url": "https://github.com/zswang/jhtmls/issues"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "mocha": "^2.0.1",
    "istanbul": "^0.3.17",
    "should": "^4.1.0",
    "uglify-js": "^2.4.15",
    "jdists": "^1.3.3",
    "jshint": "^2.5.8",
    "jstrs": "^0.0.1"
  },
  "scripts": {
    "_update_version": "jdists version.jdists",
    "_dist": "jdists src/jhtmls.js -o jhtmls.js",
    "_compress": "uglifyjs jhtmls.js -o jhtmls.min.js -p 5 -c -m",
    "example": "jdists example.jdists.js -o test/example.js",
    "mocha": "mocha",
    "test": "istanbul cover --hook-run-in-context node_modules/mocha/bin/_mocha -- -R spec",
    "dist": "npm run _update_version && npm run _dist && npm run _compress && npm run example && npm run test",
    "lint": "jshint src/*.js"
  },
  "files": [
    "jhtmls.js",
    "README.md"
  ]
}